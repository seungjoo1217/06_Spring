import lombok.extern.slf4j.Slf4j;

@Controller
@Slf4j
@RequestMapping("mamber")
public class MemberController {

	@Autowired // 의존성 주입(DI)
	private MemberService service;
	
	
	/* [로그인]
	 * - 특정 사이트에 아이디/비밀번호 등을 입력해서
	 * 해당 정보가 있으면 조회/서비스 이용
	 * 
	 * - 로그인 한 정보를 session에 기록
	 * 
	 * 
	 * 
	 * 
	 * */
}
